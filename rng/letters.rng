   <define name="tei_relatedItem">
      <element name="relatedItem">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains or references some other bibliographic item which is related to the present one in some specified manner, for example as a constituent or alternative version of it. [3.12.2.7. Related Items]</a:documentation>
         <optional>
            <choice>
               <ref name="tei_model.biblLike"/>
               <ref name="tei_model.ptrLike"/>
            </choice>
         </optional>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="tei_math-relatedItem-type-subtype-check-constraint-rule-45">
            <rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:relatedItem[normalize-space(@subtype)]">
               <report test="not(@type) or normalize-space(@type)=''">ERROR: You must supply a valid @type (e.g. draft, copy, translation).</report>
               <report test="@type='publication' and normalize-space(@subtype) !=''">ERROR: @type='publication' usually doesn't have @subtype (one of them is likely wrong).</report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="tei_math-relatedItem-check_file_pointer-constraint-rule-46">
            <rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:xi="http://www.w3.org/2001/XInclude"
                  context="tei:relatedItem[matches(@target,'.+\.xml$')]">
               <let name="document_uri" value="replace(document-uri(/),'/[^/]+$','/')"/>
               <assert test="doc-available(concat($document_uri,@target))">ERROR: <name/> points to a non-existent file (<value-of select="@target"/>)</assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="tei_math-relatedItem-targetorcontent1-constraint-report-38">
            <rule context="tei:relatedItem">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="@target and count( child::* ) &gt; 0">
If the @target attribute on <sch:name/> is used, the
relatedItem element must be empty</sch:report>
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="@target or child::*">A relatedItem element should have either a 'target' attribute
        or a child element to indicate the related bibliographic item</sch:assert>
            </rule>
         </pattern>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">values are "copy", "draft", "publication", and "translation".</a:documentation>
            <choice>
               <value>copy</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>draft</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>publication</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>translation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
         <optional>
            <attribute name="subtype">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(subtype) values should be a short canonical one word abbreviation for the institution (e.g. "CUL", "NHM", "Liverpool".</a:documentation>
               <data type="token">
                  <param name="pattern">[^\p{C}\p{Z}]+</param>
               </data>
            </attribute>
         </optional>
         <optional>
            <attribute name="target">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to either to another letter file in our collection (eg. ../letters/DCP-LETT-1.xml or ../drafts/DCP-LETT-1234.xml).</a:documentation>
               <data type="string">
                  <param name="pattern">.+\.xml(#\S+)*</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
