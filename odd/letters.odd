               <!--Element definitions-->
               
               <!--Elements from header-->
               
               <elementSpec module="header" ident="abstract" mode="change">
                  <desc versionDate="2012-12-27" xml:lang="en">contains a summary of the letter.</desc>
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <abstract>
                           <p>The third and last part of the <hi rend="italic">Geology</hi> [<hi rend="italic">South America</hi>] will be published in a few days. Apologises for not sending JSH the other volumes.</p>
                           <p>Has attended Southampton [BAAS] meeting.</p>
                        </abstract>
                     </egXML>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="authority" mode="change">
                  
                  <classes mode="change">
                     <memberOf key="att.canonical" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <authority>Darwin Correspondence Project</authority>
                     </egXML>
                  </exemplum>
                  <remarks xml:lang="en" versionDate="2024-03-02"><p><hi rend="bold">NB:</hi> The value in <gi>authority</gi> wil be used as your ‘Contributor‘ name in the browsing facets on Epsilon.</p></remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="availability" mode="change">
                  <classes mode="change">
                     <memberOf key="att.declarable" mode="delete"/>
                  </classes>
                  <attList>
                     <attDef ident="status" usage="opt" mode="change">
                        <valList type="closed" mode="replace">
                           <valItem ident="restricted"/>
                        </valList>
                     </attDef>
                     <attDef ident="xml:id" usage="opt" mode="change">
                        <desc xml:lang="en" versionDate="2022-07-05">identifies the specific rights being discussed</desc>
                        <valList type="closed" mode="replace">
                           <valItem ident="displayImageRights"/>
                           <valItem ident="downloadImageRights"/>
                           <valItem ident="metadataRights"/>
                        </valList>
                     </attDef>
                     <attDef ident="ana" mode="delete"/>
                     <attDef ident="cert" mode="delete"/>
                     <attDef ident="change" mode="delete"/>
                     <attDef ident="copyOf" mode="delete"/>
                     <attDef ident="corresp" mode="delete"/>
                     <attDef ident="exclude" mode="delete"/>
                     <attDef ident="facs" mode="delete"/>
                     <attDef ident="n" mode="delete"/>
                     <attDef ident="next" mode="delete"/>
                     <attDef ident="prev" mode="delete"/>
                     <attDef ident="rend" mode="delete"/>
                     <attDef ident="rendition" mode="delete"/>
                     <attDef ident="resp" mode="delete"/>
                     <attDef ident="sameAs" mode="delete"/>
                     <attDef ident="select" mode="delete"/>
                     <attDef ident="source" mode="delete"/>
                     <attDef ident="style" mode="delete"/>
                     <attDef ident="synch" mode="delete"/>
                     <attDef ident="xml:base" mode="delete"/>
                     <attDef ident="xml:lang" mode="delete"/>
                     <attDef ident="xml:space" mode="delete"/>
                  </attList>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <availability status="restricted">
                           <p>Available under license only</p>
                        </availability>
                     </egXML>
                  </exemplum>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <availability xml:id="metadataRights" status="restricted">
                           <licence target="http://creativecommons.org/licenses/by-nc-nd/3.0/">This metadata is licensed under a Creative Commons Attribution-NonCommercial-NoDerivs 3.0 Unported License.</licence>
                        </availability>
                     </egXML>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="change" mode="change">
                  <desc versionDate="2011-10-31" xml:lang="en">documents a change or set of changes made during the production
                     of a source document, or during the revision of an electronic file.</desc>
                  <classes mode="change">
                     <memberOf key="att.ascribed" mode="delete"/>
                  </classes>
                  <attList>
                     <attDef ident="rend" mode="change">
                        <!--NEW-->
                     </attDef>
                     <attDef ident="status" usage="opt" mode="change">
                        <valList type="semi" mode="change">
                           <valItem ident="transcribed"/>
                           <valItem ident="copyedited"/>
                           <valItem ident="proofed"/>
                           <valItem ident="reviewed"/>
                           <valItem ident="released"/>
                           <valItem ident="cancelled"/>
                        </valList>
                     </attDef>
                     <attDef ident="type" usage="opt" mode="change">
                        <desc versionDate="2014-06-09" xml:lang="en">documents significant stages in the creation, transcription, checking, processing and clearance for release of an electronic file.</desc>
                        <valList type="closed" mode="replace">
                           <valItem ident="footnotes"/>
                           <valItem ident="letter"/>
                           <valItem ident="translation"/>
                        </valList>
                     </attDef>
                     <attDef ident="when" usage="opt" mode="change">
                        <!--NEW-->
                     </attDef>
                     <attDef ident="ana" mode="delete"/>
                     <attDef ident="calendar" mode="delete"/>
                     <attDef ident="cert" mode="delete"/>
                     <attDef ident="change" mode="delete"/>
                     <attDef ident="copyOf" mode="delete"/>
                     <attDef ident="corresp" mode="delete"/>
                     <attDef ident="datingMethod" mode="delete"/>
                     <attDef ident="datingPoint" mode="delete"/>
                     <attDef ident="exclude" mode="delete"/>
                     <attDef ident="facs" mode="delete"/>
                     <attDef ident="from" mode="delete"/>
                     <attDef ident="from-custom" mode="delete"/>
                     <attDef ident="from-iso" mode="delete"/>
                     <attDef ident="n" mode="delete"/>
                     <attDef ident="next" mode="delete"/>
                     <attDef ident="notAfter" mode="delete"/>
                     <attDef ident="notAfter-custom" mode="delete"/>
                     <attDef ident="notAfter-iso" mode="delete"/>
                     <attDef ident="notBefore" mode="delete"/>
                     <attDef ident="notBefore-custom" mode="delete"/>
                     <attDef ident="notBefore-iso" mode="delete"/>
                     <attDef ident="period" mode="delete"/>
                     <attDef ident="prev" mode="delete"/>
                     <attDef ident="rendition" mode="delete"/>
                     <attDef ident="resp" mode="delete"/>
                     <attDef ident="sameAs" mode="delete"/>
                     <attDef ident="select" mode="delete"/>
                     <attDef ident="source" mode="delete"/>
                     <attDef ident="style" mode="delete"/>
                     <attDef ident="subtype" mode="delete"/>
                     <attDef ident="synch" mode="delete"/>
                     <attDef ident="target" mode="delete"/>
                     <attDef ident="to" mode="delete"/>
                     <attDef ident="to-custom" mode="delete"/>
                     <attDef ident="to-iso" mode="delete"/>
                     <attDef ident="when-custom" mode="delete"/>
                     <attDef ident="when-iso" mode="delete"/>
                     <attDef ident="xml:base" mode="delete"/>
                     <attDef ident="xml:id" mode="delete"/>
                     <attDef ident="xml:lang" mode="delete"/>
                     <attDef ident="xml:space" mode="delete"/>
                  </attList>
                  <exemplum xml:lang="en">
                     <p>You should create a new <gi>change</gi> element with required <att>when</att> attribute giving the date, and content giving a natural-language account of each significant revision. The person who made it goes in a <gi>name</gi> element with an <att>xml:id</att> value that will be assigned by the senior editors when someone starts work, but is normally based on that person's first initial plus her or his surname, e.g. <val>mhawkins</val>, <val>lsmith</val> or <val>sevans</val>.</p>
                     <p>Proofreading a file counts as a <gi>change</gi> even if the proofreader has not in fact made any changes to it since it records that the files has undergone a particular part of our editorial process.</p>
                     <p>If the <att>xml:id</att> value of any of the people working on the document has previously been declared, subsequent occurrences of her or his <gi>name</gi> take a <att>sameAs</att> value of # followed by the relevant string.</p>
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <change when="2015-10-02" status="draft">Transcribed by <name xml:id="mjhawkins">Michael Hawkins</name></change>
                        <change when="2015-10-12" status="proofed">Proofed by <name xml:id="lsmith">Liz Smith</name></change>
                        <change when="2015-10-15" status="released">Proofed by <name sameAs="#lsmith">Liz Smith</name></change>
                     </egXML>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="correspAction" mode="change">
                  <desc versionDate="2014-09-05" xml:lang="en">contains structured and regularised metadata regarding the correspondent's name, date and place of the sender or recipient of the letter</desc>
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                     <memberOf key="att.sortable" mode="delete"/>
                  </classes>
                  <attList>
                     <attDef ident="subtype" mode="change">
                        <!--NEW-->
                     </attDef>
                     <attDef ident="type" usage="req" mode="change">
                        <valList type="closed" mode="add">
                           <valItem ident="sent">
                              <desc versionDate="2015-02-09" xml:lang="en">information about the sending of the letter</desc>
                           </valItem>
                           <valItem ident="received">
                              <desc versionDate="2015-02-09" xml:lang="en">information about the receipt of the letter</desc>
                           </valItem>
                        </valList>
                     </attDef>
                  </attList>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <correspAction type="sent">
                           <persName>Darwin, C. R.</persName>
                           <placeName>Down</placeName>
                           <date when="1862-11-24">24 <supplied resp="#mjhawkins">Nov 1862</supplied></date>
                        </correspAction>
                     </egXML>
                  </exemplum>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <correspAction type="received">
                           <persName>Hooker, J. D.</persName>
                        </correspAction>
                     </egXML>
                  </exemplum>
                  <remarks versionDate="2014-04-20" xml:lang="en">
                     <p>Every record must have one <name/>/@type='sent'.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="correspDesc" mode="change">
                  <desc versionDate="2016-05-07" xml:lang="en">contains the regularised metadata regarding the sending and receipt of the letter.</desc>
                  <classes mode="change">
                     <memberOf key="att.canonical" mode="delete"/>
                     <memberOf key="att.declarable" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                     <memberOf key="att.typed" mode="delete"/>
                  </classes>
                  <constraintSpec ident="correspAction-required" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:correspDesc">
                           <assert test="count(./tei:correspAction[@type='sent']) eq 1">ERROR: There must be at least one correspAction/@type='sent' in a letter</assert>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <correspDesc>
                           <correspAction type="sent">
                              <persName>Darwin, C. R.</persName>
                              <placeName>Down</placeName>
                              <date when="1862-11-24">24 Nov 1862</date>
                           </correspAction>
                           <correspAction type="received">
                              <persName>Hooker, J. D.</persName>
                           </correspAction>
                        </correspDesc>
                     </egXML>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="editorialDecl" mode="change">              
                  <classes mode="change">
                     <memberOf key="att.declarable" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <editorialDecl>
                           <p>This transcript was produced as a side-product of thesis research and may not have been subject to the extensive proofreading of a published critical edition.</p>
                        </editorialDecl>
                     </egXML>
                  </exemplum>
                  <remarks xml:lang="en" versionDate="2022-10-11">
<p>A declaration of editorial principles and practices that is visible to the reader.</p>
                     <p><hi rend="bold">NB:</hi> The contents of this tag appear underneath the letter transcription in Epsilon.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="encodingDesc" mode="change">
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <encodingDesc>
                           <ab type="transcription_quality">Draft transcription</ab>
                           <editorialDecl>
                              <p>This transcript was produced as a side-product of the work of the <ref target="https://www.darwinproject.ac.uk/">Darwin Correspondence Project</ref> and may not have been proofread to the DCP’s usual standards.</p>
                           </editorialDecl>
                        </encodingDesc>
                     </egXML>
                  </exemplum>
                  <remarks xml:lang="en" versionDate="2022-10-11">
                     <p/>
                  </remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="fileDesc" mode="change">
                  <desc xml:lang="en" versionDate="2022-07-05">contains information about the electronic file and the physical letter’s source(s).</desc>
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <fileDesc>
                           <titleStmt>
                              <title>From Wilhelm&#160;Pfeffer&#160;&#160; 6&#160;November&#160;1881</title>
                           </titleStmt>
                           <publicationStmt>
                              <!-- -->
                           </publicationStmt>
                           <notesStmt>
                              <!-- -->
                           </notesStmt>
                           <sourceDesc>
                              <!-- -->
                           </sourceDesc>
                        </fileDesc>
                     </egXML>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="funder" mode="change">
                  <classes mode="change">
                     <memberOf key="att.canonical" mode="delete"/>
                     <memberOf key="att.datable" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples"/>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="idno" mode="change">
                  <classes mode="change">
                     <memberOf key="att.datable" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                     <memberOf key="att.sortable" mode="delete"/>
                  </classes>
                  <attList>
                     <attDef ident="type" mode="change">
                        <!--NEW-->
                     </attDef>
                     <attDef ident="subtype" mode="delete"/>
                  </attList>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <idno>GSL/COM/P/4/2/49</idno>
                     </egXML>
                  </exemplum>
                  <remarks xml:lang="en" versionDate="2022-07-05">
                     <p>When placed within <gi>msIdentifier</gi>, <gi>idno</gi> specifies the classmark/identifier of the original manuscript/document.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec ident="licence" module="header" mode="change">
                  <classes mode="change">
                     <memberOf key="att.datable" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <attList>
                     <attDef ident="target" mode="change">
                        <!--NEW-->
                     </attDef>
                     <attDef ident="evaluate" mode="delete"/>
                     <attDef ident="targetLang" mode="delete"/>
                  </attList>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <licence target="http://creativecommons.org/licenses/by-nc/4.0/">This work is licensed under CC BY-NC 4.0. To view a copy of this license, visit <ref target="http://creativecommons.org/licenses/by-nc/4.0/">http://creativecommons.org/licenses/by-nc/4.0/</ref></licence>
                     </egXML>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="notesStmt" mode="change">
                  <desc xml:lang="en" versionDate="2022-07-05">functions as a container for one or more notes on the document.</desc>
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <notesStmt>
                           <note type="endorsement" subtype="letter">Charles Darwin Aug<hi rend="superscript">st</hi> 4 1828&#160;WDF</note>
                           <note type="physdesc" n="ALS">4pp</note>
                           <relatedItem type="copy" subtype="CUL"><bibl>CUL DAR 144: 137</bibl></relatedItem>
                           <relatedItem type="publication">
                              <bibl><hi rend="italic">Correspondence</hi> 1: 62</bibl>
                           </relatedItem>
                        </notesStmt>
                     </egXML>
                  </exemplum>
                  <remarks xml:lang="en" versionDate="2022-07-05">
<p>Currently this field is used to record additional versions of a letter - copies, or alternate publications.</p>
                     <p><hi rend="bold">NB:</hi> <gi>relatedItem</gi> can continue to be used but there's a possibility that its contents might be migrated into a more semantically appropriate place within the header.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="profileDesc" mode="change">
                  <desc xml:lang="en" versionDate="2022-07-05">describes the correspondents, the summary of the letter and any associated keywords.</desc>
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <profileDesc>
                           <correspDesc>
                              <correspAction type="sent">
                                 <persName>Darwin, C. R.</persName>
                                 <placeName>Down</placeName>
                                 <date when="1881-12-28">28 Dec 1881</date>
                              </correspAction>
                              <correspAction type="received">
                                 <persName>Baily, Walter</persName>
                              </correspAction>
                           </correspDesc>
                           <abstract>
                              <p>Statement about a beetle-hunting worm is new to CD.</p>
                           </abstract>
                        </profileDesc>
                     </egXML>
                  </exemplum>
               </elementSpec>
               
               <elementSpec module="header" ident="publicationStmt" mode="change">
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <publicationStmt>
                           <authority>Darwin Correspondence Project</authority>
                           <publisher>Cambridge University Press</publisher>
                           <pubPlace>Cambridge</pubPlace>
                           <availability status="restricted">
                              <p>Available under license only</p>
                           </availability>
                           <!-- ... -->
                        </publicationStmt>
                     </egXML>
                  </exemplum>
<remarks xml:lang="en" versionDate="2022-07-05">
<p>Information on the publication of the text, whether on Epsilon or elsewhere.</p>
                     
                  </remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="revisionDesc" mode="change">
                  <desc xml:lang="en" versionDate="2022-07-05">records significant work done on the electronic file, with dates and the identities of the editors who have done the work.</desc>
                  <classes mode="change">
                     <memberOf key="att.docStatus" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <revisionDesc>
                           <change when="2015-10-02" status="draft">Transcribed by <name xml:id="jdoe">Jane Doe</name></change>
                           <change when="2015-10-12" status="proofed">Proofed by <name xml:id="another">A.N. Other</name></change>
                           <change when="2015-10-15" status="released">Proofed by <name sameAs="#another">A.N. Other</name></change>
                        </revisionDesc>
                     </egXML>
                  </exemplum>
                  <remarks xml:lang="en" versionDate="2022-07-05">
                     <p><gi>revisionDesc</gi> is not intended to provide a detailed log of all the work done on the file. If you desire something like that, I'd recommend using some source control management system, like GitHub. <name/> is intended to record significant milestones or phases of work, usually marked with on <gi>change</gi>’s <att>status</att>.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="seriesStmt" mode="change">
                  <classes mode="change">
                     <memberOf key="att.declarable" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <seriesStmt>
                           <title level="s">The Correspondence of Charles Darwin</title>
                           <biblScope unit="vol" n="01">1</biblScope>
                        </seriesStmt>
                     </egXML>
                  </exemplum>
<remarks xml:lang="en" versionDate="2022-07-05">
<p>If the edition the letter is part of forms a series, record information here about the series and the letter's place in it.</p>
</remarks>
               </elementSpec>
               
               <elementSpec module="header" ident="sourceDesc" mode="change">
                  
                  <classes mode="change">
                     <memberOf key="att.declarable" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <sourceDesc>
                           <msDesc>
                              <msIdentifier>
                                 <repository>CUL</repository>
                                 <collection>DAR</collection>
                                 <idno>115: 121, 124a, 126a</idno>
                              </msIdentifier>
                           </msDesc>
                        </sourceDesc>
                     </egXML>
                  </exemplum>
                  <remarks xml:lang="en" versionDate="2022-07-05">
                     <p>Information about the source material is recorded twice due to historical reasons.</p>
                     <p><tag>bibl type="provenance"</tag> contains the version that is displayed in <hi rend="italic">all</hi> outputs (online, print). The information in <gi>msDesc</gi> and <tag>bibl type="printedSource"</tag> is used for data processing.</p>
                  </remarks>
               </elementSpec>
               
               <elementSpec ident="teiHeader" module="header" mode="change">
                  <desc versionDate="2016-05-07" xml:lang="en">contains all the metadata pertaining to the electronic document, its original physical source as well as the correspondence details.</desc>
                  <classes mode="change">
                     <memberOf key="att.global" mode="delete"/>
                  </classes>
                  <exemplum xml:lang="en">
                     <egXML xmlns="http://www.tei-c.org/ns/Examples">
                        <teiHeader>
                           <fileDesc>
                              <!-- fileDesc contains information about the file, its source and
                                 its published versions -->
                              <titleStmt/>
                              <publicationStmt/>
                              <seriesStmt/>
                              <sourceDesc/>
                           </fileDesc>
                           <profileDesc>
                              <!-- profileDesc contains the bulk of the metadata in DCP transcriptions -->
                              <correspDesc>
                                 <!-- contains information about the correspondence exchange -->
                              </correspDesc>
                              <abstract>
                                 <!-- contains a summary of the item -->
                              </abstract>
                              <textClass>
                                 <!-- contains the categories assigned to a letter -->
                              </textClass>
                           </profileDesc>
                           <revisionDesc>
                              <!-- contains information about the changes made to the XML file -->
                           </revisionDesc>
                        </teiHeader>
                     </egXML>
                  </exemplum>
                  <remarks versionDate="2016-05-07" xml:lang="en">
                     <p>This element is generated automatically from the template file used to create XML files for new letters. However, several of its component parts will need to be modified or added by the editors, as explained in the documentation.</p>
                  </remarks>
               </elementSpec>
               
