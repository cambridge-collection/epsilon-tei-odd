               <elementSpec module="core" ident="gap" mode="change">
                  <desc versionDate="2008-01-24" xml:lang="en">indicates a point in the letter where the text remains wholly indecipherable and could be transcribed at all. When <tag>reason="illeg"</tag>, it renders as italicised square braces surrounding the contents of <att>extent</att>. When <tag>reason="damage"</tag> it renders as pointed brackets surrounding either two or four non-breaking spaces depending on the value of <att>unit</att>.</desc>
                  <classes mode="change">
                     <memberOf key="att.editLike" mode="delete"/>
                     <memberOf key="att.global" mode="delete"/>
                     <memberOf key="att.timed" mode="delete"/>
                  </classes>
                  <constraintSpec ident="gap-reason-check" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:gap">
                           <report test="@reason = 'illeg'and @unit">ERROR: @unit cannot be used when @reason='illeg'</report>
                           <report test="@reason = 'damage'and @extent">ERROR: @extent cannot be used when @reason='damage'</report>
                           <report test="@reason = 'illeg' and not(normalize-space(@extent))">ERROR: @extent needs to have a value, which will be displayed in italic between square braces, when @reason='illeg'</report>
                           <report test="@reason = 'damage' and not(normalize-space(@unit))">ERROR: @unit needs to have a value of partialWord or Word when @reason='damage'</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="gap-extent-check" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:gap[@extent]">
                           <report test="@unit">ERROR: @unit cannot be used with @extent</report>
                           <report test="@reason != 'illeg'">ERROR: @extent must be used with @reason='illeg'</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <constraintSpec ident="gap-unit-check" scheme="schematron">
                     <constraint>
                        <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:gap[@unit]">
                           <report test="@unit and not(@reason='damage')">ERROR: @unit must be used with @reason='damage'</report>
                        </rule>
                     </constraint>
                  </constraintSpec>
                  <attList>
                     <attDef ident="extent" mode="change" usage="opt">
                        <desc versionDate="2014-05-21" xml:lang="en">records the extent of the omission. This is the text that would appear in italic between italicised square braces.</desc>
                     </attDef>
                     <attDef ident="reason" usage="req" mode="change">
                        <desc versionDate="2005-01-14" xml:lang="en">indicates why the material could not be transcribed. Values are "<val>damage</val>" (renders with '〈' and '〉') or "<val>illeg</val>" (renders with italicised square braces).</desc>
                        <valList type="closed" mode="replace">
                           <valItem ident="illeg">
                              <desc xml:lang="en" versionDate="2016-07-18">text is illegible for any reason other than 'damage'. Used when italicised [some text] is desired.</desc>
                           </valItem>
                           <valItem ident="damage">
                              <desc xml:lang="en" versionDate="2016-07-18">text has been rendered illegible by damage. Use when two or foud non-breaking spaces between '〈' and '〉' are required.</desc>
                           </valItem>
                        </valList>
                     </attDef>
                     <attDef ident="unit" mode="change" usage="opt">
                        <desc versionDate="2014-05-21" xml:lang="en">defines the 'units' the undecipherable text. Permitted values: "<val>partialWord</val>" (renders as two non-breaking spaces), and "<val>word</val>" (renders as four non-breaking spaces). Only to be used when <tag>reason="damaged"</tag></desc>
                        <valList type="closed" mode="replace">
                           <valItem ident="partialWord"/>
                           <valItem ident="word"/>
                        </valList>
                     </attDef>
                     <attDef ident="agent" mode="delete"/>
